{
  "name": "great-bear-hasura",
  "version": "0.0.1",
  "description": "An online store API example using Hasura",
  "main": "src/server.ts",
  "repository": "https://github.com/sastraxi/great-bear-hasura",
  "author": "Cameron Gorrie <sastraxi@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "hasura:console": "(SECRET=`dotenv -p HASURA_SECRET`; cd hasura && hasura console --access-key $SECRET)",
    "hasura:migrate:apply": "(SECRET=`dotenv -p HASURA_SECRET`; cd hasura && hasura migrate apply --access-key $SECRET)",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "apollo-server-express": "^2.4.0",
    "bcrypt": "^3.0.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "knex": "^0.16.3",
    "lodash": "^4.17.13",
    "moment": "^2.24.0",
    "objection": "^1.5.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "stripe": "^6.23.1",
    "ts-node": "^8.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.11",
    "@types/knex": "^0.15.1",
    "@types/lodash": "^4.14.120",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/stripe": "^6.19.7",
    "nodemon": "^1.18.9",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.3.1"
  }
}
